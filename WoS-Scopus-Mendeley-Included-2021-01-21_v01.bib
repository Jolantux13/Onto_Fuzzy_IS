doi = {10.1142/S0218488512400235},
issn = {0218-4885},
journal = {INTERNATIONAL JOURNAL OF UNCERTAINTY FUZZINESS AND KNOWLEDGE-BASED SYSTEMS},
keywords = {Genetic fuzzy markup language; genetic fuzzy syste},
number = {2, SI},
pages = {247--278},
publisher = {WORLD SCIENTIFIC PUBL CO PTE LTD},
title = {{A NOVEL GENETIC FUZZY MARKUP LANGUAGE AND ITS APPLICATION TO HEALTHY DIET ASSESSMENT}},
type = {Article},
volume = {20},
year = {2012}
}
@article{Devignes2012245,
abstract = {Functional classification aims at grouping genes according to their molecular function or the biological process they participate in. Evaluating the validity of such unsupervised gene classification remains a challenge given the variety of distance measures and classification algorithms that can be used. We evaluate here functional classification of genes with the help of reference sets: KEGG (Kyoto Encyclopaedia of Genes and Genomes) pathways and Pfam clans. These sets represent ground truth for any distance based on GO (Gene Ontology) biological process and molecular function annotations respectively. Overlaps between clusters and reference sets are estimated by the F-score method. We test our previously described IntelliGO semantic distance with hierarchical and fuzzy C-means clustering and we compare results with the state-of-the-art DAVID (Database for Annotation Visualisation and Integrated Discovery) functional classification method. Finally, study of best matching clusters to reference sets leads us to propose a set-difference method for discovering missing information. Copyright {\textcopyright} 2012 Inderscience Enterprises Ltd.},
