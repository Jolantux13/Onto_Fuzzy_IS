title = {{Exploring eWOM in online customer reviews: Sentiment analysis at a fine-grained level}},
type = {Article},
volume = {81},
year = {2019}
}
@article{ISI:000472002600001,
abstract = {In order to quantify the effects of different body parts on sitting
comfort, an ontology model for sitting comfort in humanmachine system
was established. In light of the subjectivity and arbitrariness issue of
AHP, as well as the characteristic of entropy method be vulnerable to
disturbed by extremum, a new weighting method was implemented which
