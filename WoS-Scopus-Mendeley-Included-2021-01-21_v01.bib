keywords = {attribute generalization; concept hierarchies; dat},
number = {6},
pages = {767--780},
publisher = {IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC},
title = {{A multicriteria approach to data summarization using concept ontologies}},
type = {Article},
volume = {14},
year = {2006}
}
@inproceedings{ISI:000237649800002,
abstract = {This paper introduces an ontology-based context model in ubiquitous
