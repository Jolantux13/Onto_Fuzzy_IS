publisher = {IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC},
title = {{Modeling gene expression networks using fuzzy logic}},
type = {Article},
volume = {35},
year = {2005}
}
@inproceedings{ISI:000232722500029,
abstract = {Fuzzy ontology mapping is important for handling uncertain knowledge on
the semantic web. However, current ontology mapping technologies are not
sufficient for fuzzy ontologies. This paper proposes a framework of
mapping fuzzy concepts between fuzzy ontologies. It applies the
approximate concept mapping approach, extends atom fuzzy concept sets
and defines the least upper bounds to reduce the searching space. It
resolves the mapping problem of fuzzy concepts into finding the
simplified least upper bounds for atom fuzzy concepts, and gives an
algorithm for searching the simplified least upper bounds, which is fast
