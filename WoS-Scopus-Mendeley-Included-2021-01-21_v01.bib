journal = {International Journal of Image and Data Fusion},
keywords = {Alluvial fans; Geomorphometry; Multi scale analys,California; Death Valley; Nevada; United States,Image analysis,Image enhancement; Image processing; Image segment,accuracy assessment; alluvial fan; data set; digi},
number = {4},
pages = {309--331},
publisher = {Taylor and Francis Ltd.},
title = {{GEOBIA-based identification of alluvial fans and bajadas through geomorphometry, image analysis and fuzzy ontology}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85021273014{\&}doi=10.1080{\%}2F19479832.2017.1342707{\&}partnerID=40{\&}md5=19e24bb12b43be1e54e7168e24fb43de},
