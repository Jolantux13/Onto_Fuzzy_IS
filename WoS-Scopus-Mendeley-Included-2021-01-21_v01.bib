@article{ISI:000263421700007,
abstract = {This paper presents FROM, the fuzzy relational ontological model, a
novel approach to encode knowledge for information retrieval
applications based upon a fuzzy set framework that consider more generic
concepts differently from specific terms. Besides the model itself, the
paper also presents a retrieval algorithm that exploits FROM features
