}
@article{ISI:000236036400003,
abstract = {A Genetic Fuzzy Agent (GFA) using the ontology model for Meeting
Scheduling System (MSS) is presented in this paper. The ontology model
includes the Fuzzy Meeting Scheduling Ontology (FMSO) and the Fuzzy
Personal Ontology (FPO) that can support to construct the knowledge base
