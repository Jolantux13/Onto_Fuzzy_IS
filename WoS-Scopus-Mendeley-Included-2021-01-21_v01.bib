@article{Fang20071000,
abstract = {Today quite a lot of 'dynamic fuzzy' data exist in semantic web to enable representing and reasoning for fuzzy ontology with expressive fuzzy knowledge. The research on them attract more and more attention and an approach with dynamic fuzzy logic theory on the Semantic Web came to being. In the past, Ontology languages is based on description logics(DL) with expressive enough, but they lack the capacity encode and reason the imprecise knowledge. Then we extend the DL language by means of dynamic fuzzy logic, defining formulas and inference rules of propositional logic, and express the vague ontology with the dynamic method. It presents more expressive power than traditional methods.},
