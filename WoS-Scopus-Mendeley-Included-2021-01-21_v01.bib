@article{Herrera-Viedma2006105,
abstract = {In this paper we define a model of a web multi-agent system that combines the use of Semantic Web technologies (like RDF and OWL languages to describe resources and define ontologies respectively) together with the application of user profiles to carry out its activity. It is designed using fuzzy linguistic techniques which allow dealing with information in a user-friendly way. The system activity is developed in two phases: retrieval phase to gather the documents from the Web, and feedback phase to update the elements used to filter the documents, i.e., user profiles and recommendation values. We focus on the analysis of the feedback phase. With this multi-agent system model the filtering possibilities are increased and, consequently, also the retrieval capabilities on the Web. {\textcopyright} 2006 Springer-Verlag Berlin Heidelberg.},
