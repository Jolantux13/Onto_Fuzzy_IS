@article{Zhao20131679,
abstract = {Recently, there is an increasing interest to extend Web Ontology Language (OWL) to represent uncertain knowledge. Fuzzy variables of description logics were introduced in order to deal with applications where standard definitions are not possible. Besides, description logics does not support customized data types and customized datatype predicates. In this paper, we generalize existing proposals and show how to reason with a fuzzy extension of the logic SROIQ(G) DL, the logic behind the language OWL 2. We present a novel kind of fuzzy description logic f-SROIQ DL, extends existing OWL 2 language by encoding fuzzy constructors, axioms and constraints and maps semantics of these new vocabularies to fuzzy description logic, that supports customized fuzzy data type predicate. Copyright {\textcopyright} 2013 Binary Information Press.},
