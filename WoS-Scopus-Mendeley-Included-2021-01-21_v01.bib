}
@article{ISI:000260564500004,
abstract = {A novel fuzzy ontology for project planning is proposed in this paper.
The domain knowledge of the fuzzy ontology is retrieved from the process
area of project planning based on Capability Maturity Model Integration
(CMMI). We propose an ontology-based fuzzy agent for CMMI project
planning, including a project planning ontology and a TSK-based project
cost estimation mechanism, to estimate the total project cost. Combined
the fuzzy inference, CMMI, and the ontology, the fuzzy agent infers the
total project cost based on the predefined ontology and then stores the
results to the project estimation repository. The simulation results
