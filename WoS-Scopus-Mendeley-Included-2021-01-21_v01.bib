journal = {Web Intelligence and Agent Systems},
keywords = {Data description; Database systems; Formal logic;,Description logic; ER-models; Fuzzy database; fuzz,Ontology},
number = {4},
pages = {397--412},
title = {{Formal semantics-preserving translation from fuzzy ER model to fuzzy OWL DL ontology}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-78650322137{\&}doi=10.3233{\%}2FWIA-2010-0199{\&}partnerID=40{\&}md5=34a58ec1fe0c452c768d3b7261d251df},
volume = {8},
year = {2010}
}
