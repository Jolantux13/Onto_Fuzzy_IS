title = {{Decision making with a fuzzy ontology}},
type = {Article},
volume = {16},
year = {2012}
}
@article{ISI:000309553200011,
abstract = {To improve the formal integration of verbally given spatial information
in a geographical information system, a methodology for converting such
information to a geographical representation was developed. This was
applied to examples of a spatial-related analysis of natural language
text messages in the domain of disaster management. This article
