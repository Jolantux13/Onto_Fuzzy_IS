@article{Zhai20111855,
abstract = {Ontology is the basis of knowledge modeling on the Semantic Web, and fuzzy ontology is an extension of domain ontology for solving the uncertainty problems. Ontology-based knowledge modeling of product data management (PDM) is meaningful for product design and trade etc. In order to handle fuzzy phenomenon and uncertainty of product knowledge, this paper proposes a series of fuzzy ontology models that consists of fuzzy domain ontology and linguistic variable ontologies. The fuzzy ontology for product knowledge was constructed. To achieve fuzzy semantic retrieval, the semantic query expansion in SPARQL query language was established by semantic relations between fuzzy concepts defined in linguistic variable ontologies. The application shows that the extended query can return all results which satisfy query requirement at semantic level without upgrading current main search algorithm, and this research facilitates the fuzzy knowledge sharing for PDM and the semantic retrieval through fuzzy concepts on the Semantic Web. {\textcopyright} 2011 American Scientific Publishers. All rights reserved.},
