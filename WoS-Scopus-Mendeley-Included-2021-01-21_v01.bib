@article{ISI:000236036400006,
abstract = {This paper presents an ontology-based holonic diagnostic system (OHDS)
that combines the holonic paradigm with multi-agent system technology
and ontology design, for the organization of unstructured biomedical
research into structured disease information. We use ontologies as
