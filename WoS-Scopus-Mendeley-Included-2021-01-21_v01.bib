isbn = {3540712577; 9783540712572},
issn = {14349922},
journal = {Studies in Fuzziness and Soft Computing},
pages = {193--218},
title = {{Fuzzy logic and ontology-based information retrieval}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-34250686203{\&}doi=10.1007{\%}2F978-3-540-71258-9{\_}10{\&}partnerID=40{\&}md5=ab6e10709babc8a6cb8cb43d0db06c7d},
volume = {215},
year = {2007}
