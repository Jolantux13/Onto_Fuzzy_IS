publisher = {American Scientific Publishers},
title = {{An efficient technique for the automatic generation of ontology based on students behavior analysis using optimal Fuzzy-GSO algorithm}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85027977902{\&}doi=10.1166{\%}2Fjctn.2017.6765{\&}partnerID=40{\&}md5=01910609a5a4dd3420a4b1cfd46633f8},
