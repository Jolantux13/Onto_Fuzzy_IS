title = {{Knowledge logistics as an intelligent service for healthcare}},
type = {Article; Proceedings Paper},
volume = {44},
year = {2005}
}
@inproceedings{ISI:000229371400022,
abstract = {Nowadays, Internet is generally useful for intelligent information
