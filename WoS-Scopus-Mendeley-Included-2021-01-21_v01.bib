journal = {Transportation Research Part C: Emerging Technologies},
keywords = {Accidents; Advanced traveler information systems;,City-feature polarity; Fuzzy ontology; Intelligen,Traffic congestion,fuzzy mathematics; intelligent transportation sys},
pages = {33--48},
publisher = {Elsevier Ltd},
title = {{Fuzzy ontology-based sentiment analysis of transportation and city feature reviews for safe traveling}},
