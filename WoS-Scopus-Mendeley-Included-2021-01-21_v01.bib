title = {{Social Profiling: A Review, Taxonomy, and Challenges}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85068851910{\&}doi=10.1089{\%}2Fcyber.2018.0670{\&}partnerID=40{\&}md5=158f43f6cee15c7e3850dbe8de4aeb23},
