booktitle = {ARTIFICIAL INTELLIGENCE: METHODOLOGY, SYSTEMS, AND APPLICATIONS, PROCEEDINGS},
editor = {{Euzenat, J and Domingue, J}},
isbn = {3-540-40930-0},
issn = {0302-9743},
keywords = {matching color descriptions; HSL color space; fuzz},
