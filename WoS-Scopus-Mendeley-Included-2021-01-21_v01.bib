@article{Dewangan20121,
abstract = {Data mining of bearing vibration of a steam turbine based on ANFIS and genetic algorithm is brought forward, aimed at overcoming shortages of some current knowledge attaining methods. The historical fault data of steam turbine is processed with ANFIS, the ANFIS is trained via teacher's guidance, and the ANFIS is optimized by genetic algorithm. Based on the ontology of ANFIS, the data mining algorithm for classified fault diagnosis and rectification rules about bearing vibration of steam turbine is brought forward; in the given steps, computing the measurement matrix of effect; extracting rules; Computing the importance of rules; Shearing the rules by genetic algorithm. An experimental system for data mining, fault diagnosis and fault rectification bearing vibration of steam turbine based on ANFIS and genetic algorithm is implemented and its diagnosis precision is 86 {\%}. And Experiments do prove that it is a feasible to use the method to develop a system for fault diagnosis and fault remedy of bearing vibration of steam turbine, which is valuable for further study in more depth. Bearing fault diagnostics and a remedy method is presented in this paper using ANFIS and GA based on vibration data. Both time-domain and frequency-domain features are used as input data for bearing fault detection. Two separate ANFIS, one for fault diagnosis and treatment of faults, are used and called Diagnostic-ANFIS or D-ANFIS and Treatment-ANFIS or T-ANFIS respectively. Adaptive Network based Fuzzy Inference System (ANFIS) and Genetic Algorithm (GA) has been proposed to select the ANFIS model input and output parameters. Training results give the optimized ANFIS for bearing fault diagnosis and its rectification based on measured vibration data. The result is also tested with other set of bearing data to illustrate the reliability of the chosen model.},
annote = {cited By 2},
author = {Dewangan, D N and Jha, M and Qureshi, M F and Banjare, Y P},
issn = {12404543},
journal = {Advances in Modelling and Analysis B},
keywords = {Adaptive network based fuzzy inference system; Ada,Data mining,Fuzzy systems; Genetic algorithms; Optimization;},
number = {1-2},
pages = {1--21},
title = {{Real-time fault diagnostic and rectification system for bearing vibration of steam turbine by using adaptive neuro-fuzzy inference system and genetic algorithm - A novel approach}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84884547903{\&}partnerID=40{\&}md5=9bd4125908c5558ded18b99eb32f0956},
volume = {55},
year = {2012}
}
@article{Ge20121445,
abstract = {Ontology is playing an important role in knowledge management and sharing, both users and system can communicate with each other using a common understanding knowledge of a domain. This study proposes a context-based ontology construction method for extracting petroleum exploration domain information from unstructured Chinese text documents. The proposed mechanism of domain ontology construction includes four steps. First, domain documents preprocessing aims to separates the text into sentences, including a Chinese Partof-Speech (POS) Tag and a Chinese corpus extract from the HowNet. Next, the concept clustering based on the fuzzy cmeans aims to cluster concepts and instances from documents. In third step, context extraction aims to obtain the contexts. Finally, domain ontology construction aims to generate a petroleum exploration Chinese domain ontology. Experimental results show that the proposed approach can effectively construct Chinese domain ontology from unstructured text documents. This study implements a context-based ontology construction mechanism that can automatically mine domain concepts out of domain document, thereby reducing cost and burden that would be incurred in a manual construction process. {\textcopyright} 2012 ACADEMY PUBLISHER.},
