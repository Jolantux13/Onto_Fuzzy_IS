title = {{Evolutionary agents for intelligent transport systems}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-27644550689{\&}partnerID=40{\&}md5=96fb8d9ecf532aa1ee644d4c9e1b1a2d},
volume = {7},
