publisher = {WILEY-HINDAWI},
title = {{Study on the Intelligent Selection Model of Fuzzy Semantic Optimal Solution in the Process of Translation Using English Corpus}},
type = {Article},
volume = {2020},
year = {2020}
}
@article{ISI:000522384000006,
abstract = {Social networks are the preferred mean for experts to share their
knowledge and provide information. Therefore, it is one of the best
sources that can be used for obtaining data that can be used for a high
amount of purposes. For instance, determining social needs, identifying
problems, getting opinions about certain topics, ... Nevertheless, this
kind of information is difficult for a computational system to interpret
due to the fact that the text is presented in free form and that the
