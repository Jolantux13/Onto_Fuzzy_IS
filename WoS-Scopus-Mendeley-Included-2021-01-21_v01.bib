keywords = {Fuzzy cognitive maps; Fuzzy systems; Intelligent c},
number = {4},
pages = {1199--1210},
publisher = {PERGAMON-ELSEVIER SCIENCE LTD},
title = {{A dynamic fuzzy cognitive map applied to chemical process supervision}},
