issn = {14248220},
journal = {Sensors (Switzerland)},
keywords = {Activity recognition; Ambient intelligence; Conte,Artificial intelligence; Computer aided design; Hy,Ontology},
number = {10},
pages = {18131--18171},
publisher = {MDPI AG},
title = {{Handling real-world context awareness, uncertainty and vagueness in real-time human activity tracking and recognition with a fuzzy ontology-based hybrid method}},
