publisher = {World Academy of Research in Science and Engineering},
title = {{Modification of models intensive development ontologies by fuzzy logic}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85084450665{\&}doi=10.30534{\%}2Fijeter{\%}2F2020{\%}2F50832020{\&}partnerID=40{\&}md5=a56defe591c1698ece2d58e8157a678f},
